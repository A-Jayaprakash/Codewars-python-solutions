def fake_bin(x):
    str = ""
    length = len(x)
    for i in range(length):
        if x[i] == ('0'):
            str+='0'
        elif x[i] == ('1'):
            str+='0'
        elif x[i] == ('2'):
            str+='0'
        elif x[i] == ('3'):
            str+='0'
        elif x[i] == ('4'):
            str+='0'
        else:
            str+='1'
    return str
